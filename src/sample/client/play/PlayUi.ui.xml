<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:v="urn:import:plumb.client.display.ui">
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:FlowPanel>
		<g:HTMLPanel>
			<h2>Nouveau Jeu</h2>
			<hr></hr>
			<v:DisplayEditView ui:field="editView"></v:DisplayEditView>
			<g:Button styleName="{style.important}" ui:field="button"
				text="Enregistrer" />
		</g:HTMLPanel>
		<g:HTMLPanel><hr></hr><h2>Liste des Jeux</h2></g:HTMLPanel>
		<g:FlowPanel ui:field="playListContainer"></g:FlowPanel>
	</g:FlowPanel>
</ui:UiBinder> 